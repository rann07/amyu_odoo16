<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <data>
        <record id="team_department_tree_view" model="ir.ui.view">
            <field name="name">team.department.tree.view</field>
            <field name="model">team.department</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="name"/>
                    <field name="active"/>
                </tree>
            </field>
        </record>

        <record id="team_department_form_view" model="ir.ui.view">
            <field name="name">team.department.form.view</field>
            <field name="model">team.department</field>
            <field name="arch" type="xml">
                <form>
                    <group>
                        <field name="name"/>
                    </group>
                    <group>
                        <field name="active" widget="boolean_toggle"/>
                    </group>
                </form>
            </field>
        </record>

        <record id="team_department_action" model="ir.actions.act_window">
            <field name="name">Team</field>
            <field name="res_model">team.department</field>
            <field name="view_mode">tree,form</field>
        </record>

        <record id="cluster_tree_view" model="ir.ui.view">
            <field name="name">cluster.tree.view</field>
            <field name="model">cluster.department</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="name"/>
                    <field name="active"/>
                </tree>
            </field>
        </record>

        <record id="cluster_form_view" model="ir.ui.view">
            <field name="name">cluster.form.view</field>
            <field name="model">cluster.department</field>
            <field name="arch" type="xml">
                <form>
                    <group>
                        <field name="name"/>
                    </group>
                    <group>
                        <field name="active" widget="boolean_toggle"/>
                    </group>
                </form>
            </field>
        </record>

        <record id="associate_cluster_action" model="ir.actions.act_window">
            <field name="name">Cluster</field>
            <field name="res_model">cluster.department</field>
            <field name="view_mode">tree,form</field>
        </record>

        <record id="job_tree_view" model="ir.ui.view">
            <field name="name">job.tree.view</field>
            <field name="model">job.title</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="name"/>
                    <field name="active"/>
                </tree>
            </field>
        </record>

        <record id="job_form_view" model="ir.ui.view">
            <field name="name">job.form.view</field>
            <field name="model">job.title</field>
            <field name="arch" type="xml">
                <form>
                    <group>
                        <field name="name"/>
                    </group>
                    <group>
                        <field name="active" widget="boolean_toggle"/>
                    </group>
                </form>
            </field>
        </record>

        <record id="job_work_action" model="ir.actions.act_window">
            <field name="name">Job Position</field>
            <field name="res_model">job.title</field>
            <field name="view_mode">tree,form</field>
        </record>
    </data>
</odoo>
